{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "interface-name": [true, "never-prefix"],
    "quotemark": [true, "single", "jsx-double"],
    "arrow-parens": [true, "ban-single-arg-parens"],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    // rule is not stable
    "jsx-key": false,
    "jsx-no-lambda": false
  }
}
